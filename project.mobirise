              ".main-row": {
                "flex-direction": "row-reverse"
              }
            },
            ".btn": {
              "margin-left": "4px"
            },
            ".mbr-section-btn": {
              "padding-top": "1.5rem",
              "position": "relative",
              "display": "inline-block",
              "width": "100%"
            },
            ".mbr-section-title": {
              "margin-bottom": ".5rem"
            },
            ".line-bottom": {
              "border": "2px solid @lineColor",
              "display": "inline-block"
            },
            "progress[value]::-webkit-progress-bar": {
              "background": "rgba(206, 206, 206, 0.4)"
            },
            "progress::-webkit-progress-value": {
              "background": "@progressColor"
            },
            "progress[value]::-moz-progress-bar": {
              "background": "@progressColor"
            },
            "progress::-ms-fill": {
              "background": "@progressColor"
            },
            ".progressbar-number": {
              "display": "inline-block"
            },
            ".progress_value": {
              "position": "absolute",
              "display": "flex",
              "flex-direction": "column"
            },
            ".progress1 .progressbar-number:before, .progress2 .progressbar-number:before": {
              "position": "absolute",
              "right": "15px",
              "top": "0"
            },
            ".progress1 .progressbar-number:after": {
              "content": "'@{progress1}'"
            },
            ".progress2 .progressbar-number:after": {
              "content": "'@{progress2}'"
            },
            ".place-number": {
              "color": "@progressColor"
            },
            ".mbr-text": {
              "margin": "1.875rem 0 3.125rem"
            },
            ".circle-main": {
              "height": "250px"
            },
            ".circle-wrapper": {
              "position": "relative",
              "display": "flex",
              "justify-content": "center",
              "align-items": "center"
            },
            ".circle1": {
              "cx": "151px",
              "cy": "124px",
              "r": "110px",
              "fill": "transparent",
              "stroke": "@progressBgColor",
              "stroke-width": "6px"
            },
            ".circle2": {
              "cx": "-3px",
              "cy": "151px",
              "r": "110px",
              "fill": "transparent",
              "stroke": "@progressColor",
              "stroke-width": "7px",
              "stroke-dasharray": "727px",
              "transform": "rotate(-90deg) translateX(-121px)"
            },
            ".progress-circle-first": {
              "stroke-dashoffset": "~'calc(726px - (6.9px * @{progress1}))'"
            },
            ".progress-circle-second": {
              "stroke-dashoffset": "~'calc(726px - (6.9px * @{progress2}))'"
            },
            "& when (@progress1 = '0')": {
              "#l": {
                "stroke-dashoffset": "730px !important"
              }
            },
            "& when (@progress2 = '0')": {
              ".circle-main > circle.progress-circle-second": {
                "stroke-dashoffset": "730px !important"
              }
            },
            "@media (max-width: 1199px)": {
              ".circle1": {
                "cx": "128px"
              },
              ".circle2": {
                "cy": "128px"
              }
            },
            "@media (max-width: 991px)": {
              ".container": {
                "text-align": "left"
              },
              ".circle1": {
                "cx": "150px"
              },
              ".circle2": {
                "cy": "150px"
              },
              ".content-wrapper": {
                "margin-bottom": "3.125rem"
              },
              ".mbr-section-btn": {
                "padding-top": "0"
              },
              ".mbr-section-title,.mbr-text,.mbr-section-btn": {
                "text-align": "left"
              }
            },
            "@media (max-width: 768px)": {
              ".circle-secind": {
                "margin-top": "3.125rem"
              }
            },
            "@media (max-width: 576px)": {
              ".circle1": {
                "cx": "145px"
              },
              ".circle2": {
                "cy": "145px"
              }
            },
            "@media (max-width: 385px)": {
              ".circle1": {
                "cx": "130px"
              },
              ".circle2": {
                "cy": "130px"
              }
            }
          },
          "_name": "progress-bars2",
          "_customHTML": "<section class=\"progress-bars2\" group=\"Progress\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"8\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"4\">\n        \n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked>\n        <input type=\"checkbox\" title=\"Show Percent\" name=\"showPercent\" checked>\n        <input type=\"color\" title=\"Progress Color\" name=\"progressColor\" value=\"#af9f8c\">\n        <input type=\"color\" title=\"Progress BG Color\" name=\"progressBgColor\" value=\"#f4f5f6\">\n        <input type=\"range\" inline title=\"Progress 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"47\">\n        <input type=\"range\" inline title=\"Progress 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"90\">\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#a2aab1\" condition=\"showLine\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n        <input type=\"checkbox\" title=\"Media on Left/Right\" name=\"flexDirection\">\n\n        <select title=\"Circle Count\" name=\"circleCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n        </select>\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background8.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\">\n    </div>\n    <div class=\"container\">\n        <div class=\"mbr-row main-row\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-4\">\n                <div class=\"content-wrapper\">\n                    <h2 class=\"mbr-section-title align-left mbr-bold mbr-black mbr-fonts-style l-spacing3\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .line\">\n                        Create awesome\n                    </h2>\n                    <div class=\"line align-left\">\n                        <div mbr-if=\"showLine \" class=\"line-bottom\"></div>\n                    </div>\n                    <p class=\"mbr-text text mbr-fonts-style l-spacing-3\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".text, .mbr-section-btn\">\n                        Cut down the development time with drag-and-drop website builder. Drop the blocks into the page, edit content inline and publish - no technical skills required. Watch the ideo below to see the Mobirise in action.\n                    </p>\n                    <div mbr-buttons class=\"mbr-section-btn\" mbr-theme-style=\"display-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                        <a class=\"btn btn-success btn-md\" href=\"https://mobirise.com\">Details</a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12 mbr-col-lg-8':circleCount==1,\n                             'mbr-col-md-6 mbr-col-lg-4':circleCount>1}\">\n                <div class=\"circle-wrapper circle-first\">\n                    <svg class=\"circle-main\">\n                      <circle class=\"circle1\"></circle>\n                      <circle class=\"circle2 progress-circle-first\" mbr-class=\"{'super-hide': progress1 == 0}\"></circle>\n                    </svg>\n                    <div mbr-if=\"showPercent\" class=\"progress_value progress1 mbr-fonts-style\" mbr-theme-style=\"display-6\">\n                        <div class=\"place-number d-flex mbr-justify-content-center\">\n                            <div class=\"progressbar-number\">\n                            </div>\n                            <span>%</span>\n                        </div>\n                        <p class=\"progress_value-name progress2 mbr-fonts-style\" data-app-selector=\".progress_value-name\">Param name</p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\" mbr-if=\"circleCount>1\">\n                <div class=\"circle-wrapper circle-secind\">\n                    <svg class=\"circle-main\">\n                      <circle class=\"circle1\"></circle>\n                      <circle class=\"circle2 progress-circle-second\" mbr-class=\"{'super-hide': progress2 == 0}\"></circle>\n                    </svg>\n                    <div mbr-if=\"showPercent\" class=\"progress_value progress2 mbr-fonts-style\" mbr-theme-style=\"display-6\">\n                        <div class=\"place-number d-flex mbr-justify-content-center\">\n                            <div class=\"progressbar-number\">\n                            </div>\n                            <span>%</span>\n                        </div>\n                        <p class=\"progress_value-name progress2 mbr-fonts-style\" data-app-selector=\".progress_value-name\">Param name</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
