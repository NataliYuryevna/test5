          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper": {
              "& when (@showBorders)": {
                "border": "1px solid @bordersColor"
              },
              "& when not (@transparentBg)": {
                "background": "@cardBg"
              },
              "@media (max-width: 991px)": {
                "flex-wrap": "wrap"
              }
            },
            ".card-box": {
              "& when (@checkImages) and (@flexDirection)": {
                "position": "relative",
                ".card-subtitle": {
                  "padding": "0",
                  "position": "absolute",
                  "right": "0",
                  "top": "0",
                  "& when (@showBorders), not (@transparentBg)": {
                    "right": "2rem",
                    "@media (max-width: 1024px)": {
                      "right": "1.5rem"
                    }
                  }
                }
              }
            },
            "& when (@checkImages) and not (@flexDirection)": {
              ".card-img": {
                "padding": "0",
                "@media (max-width: 991px)": {
                  "width": "100%",
                  "margin-bottom": "2rem"
                },
                "& when not (@chessStyle)": {
                  "@media (min-width: 992px)": {
                    "& when not (@reverseContent)": {
                      "margin-right": "2rem"
                    },
                    "& when (@reverseContent)": {
                      "margin-left": "2rem"
                    }
                  }
                }
              },
              "& when (@chessStyle)": {
                "@media (min-width: 992px)": {
                  ".mbr-row .card": {
                    "& when not (@reverseContent)": {
                      "&:nth-child(odd) .card-img": {
                        "margin-left": "2rem"
                      },
                      "&:nth-child(even) .card-img": {
                        "margin-right": "2rem"
                      }
                    },
                    "& when (@reverseContent)": {
                      "&:nth-child(odd) .card-img": {
                        "margin-right": "2rem"
                      },
                      "&:nth-child(even) .card-img": {
                        "margin-left": "2rem"
                      }
                    }
                  }
                }
              }
            },
            ".card-img .card-btn": {
              "display": "none"
            },
            ".title": {
              "padding-bottom": "3rem",
              "@media (min-width: 768px)": {
                "padding-bottom": "4.25rem"
              },
              "@media (min-width: 1025px)": {
                "padding-bottom": "7rem"
              }
            },
            ".card.btnVisible .card-img .card-btn": {
              "& when (@flexDirection)": {
                "height": "~\"calc(100% - 1rem)\""
              }
            }
          },
