| showCardsText || checkIcons\">\n    <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#ececec\" condition=\"!transparentBg && (showCardsTitle || showCardsText || checkIcons)\">\n    <input type=\"checkbox\" title=\"Icons\" name=\"checkIcons\" checked>\n    <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"8\" step=\"0.5\" value=\"2.5\" condition=\"checkIcons\">\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\">\n        <input type=\"color\" title=\"Background Color\" value=\"#f6f6f6\" selected>\n        <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/shopbg1.jpeg\">\n    </fieldset>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title mbr-pb-4 mbr-px-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-center': titleAlign == 2,\n                        'align-right': titleAlign == 3}\">\n            <div class=\"title-block\">\n                <h3 class=\"mbr-section-title mbr-regular mbr-fonts-style\" mbr-class=\"{'mbr-pb-2': showSubtitle}\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Come Visit Us</h3>\n                <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-light\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                    Contacts Subtitle\n                </h4>\n            </div>\n        </div>\n        <div class=\"mbr-row mbr-px-2 mbr-jc-c\" mbr-class=\"{'mbr-column': !flexDirection}\" mbr-if=\"showCardsTitle || showCardsText || checkIcons\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-px-5': !flexDirection,\n                            'mbr-col-md-12 mbr-px-5': flexDirection &amp;&amp; cardsCount == 1,\n                            'mbr-col-md-6 mbr-col-lg-6 mbr-px-2': flexDirection &amp;&amp; cardsCount == 2,\n                            'mbr-col-md-6 mbr-col-lg-4 mbr-px-2': flexDirection &amp;&amp; cardsCount == 3,\n                            'mbr-col-md-6 mbr-col-lg-3 mbr-px-2': flexDirection &amp;&amp; cardsCount == 4,\n                            'align-center': cardsAlign == 2,\n                            'align-right': cardsAlign == 3,\n                            'md-pb': (flexDirection &amp;&amp; cardsCount > 2) || (!flexDirection &amp;&amp; cardsCount > 1),\n                            'lg-pb': !flexDirection &amp;&amp; cardsCount > 1,\n                            'last-child': cardsCount == 1}\">\n                <div class=\"card-wrapper mbr-flex\" mbr-class=\"{'mbr-column': checkIcons && flexDirection,\n                                 'box-shadow': showBorders || !transparentBg}\">\n                    <div class=\"card-img\" mbr-if=\"checkIcons\" mbr-class=\"{'align-center': cardsAlign == 2,\n                                     'align-right': cardsAlign == 3,\n                                     'mbr-pb-2': flexDirection}\">\n                        <span mbr-icon class=\"amp-iconfont fa-map-marker fa\"></span>\n                    </div>\n                    <div class=\"card-box\" mbr-if=\"(showCardsTitle || showCardsText)\">\n                        <h3 class=\"card-title mbr-pb-1 mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\">\n                            Address\n               